{# @var datatable AppDatatableDatatableInterface #}
{% extends 'base.vue.twig' %}
{% set title = 'User' %}

{% block template %}
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <v-btn type="submit" color="danger">
        {{ btn_text|default('Yes, delete') }}
    </v-btn>
    <v-btn rounded onclick="window.history.back();">
        <v-icon>mdi-close-circle-outline</v-icon>
        {{ btn_text|default('No, go back') }}
    </v-btn>

    {{ form_end(form) }}
{% endblock %}