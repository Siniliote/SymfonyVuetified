{% extends 'base.vue.twig'  %}
{% set title = 'Library' %}

{% block template %}
    {{ vue_store('form', vue_form(form)) }}
    <v-form action="{{ path('library_new') }}" method="post">
        <form-type :form="$store.form"></form-type>
        <v-btn type="submit" color="primary" class="mt-5">Save</v-btn>
    </v-form>
{% endblock %}

{% block vueJs %}
    <script>
        window.vue = {
            created() {
                this.$store.testA = this.$store.testA+1;
            },
        }
    </script>
{% endblock %}